<model xmlns:marshal="http://namespaces.plone.org/supermodel/marshal" xmlns:form="http://namespaces.plone.org/supermodel/form" xmlns:security="http://namespaces.plone.org/supermodel/security" xmlns="http://namespaces.plone.org/supermodel/schema">
  <schema>
    <field name="overview" type="plone.app.textfield.RichText">
      <description/>
      <title>Overview</title>
      <required>False</required>
    </field>
    <field name="image" type="plone.namedfile.field.NamedBlobImage">
      <description/>
      <required>False</required>
      <title>Image</title>
    </field>
    <field name="areas_affected" type="zope.schema.TextLine">
      <description>Countrys, Regions, separate each with a comma (,)</description>
      <title>Areas Affected</title>
      <required>False</required>
    </field>
    <field name="executing_body" type="zope.schema.TextLine">
      <description>e.g Environment Division, Ministry of Tourism and Environment, the Central Board of Health, Ministry of Health;</description>
      <title>Executing Body</title>
      <required>False</required>
    </field>
    <field name="cost_of_project" type="zope.schema.Float">
      <description>State the cost of the project in USD</description>
      <title>Cost of Project</title>
      <required>False</required>
    </field>
   <field name="project_currency" type="zope.schema.Choice"
           form:widget="z3c.form.browser.radio.RadioFieldWidget">
        <description></description>
        <title>Currency</title>
        <required>True</required>
        <values><element>EURO</element>
          <element>JMD</element>
     <element>USD</element>
     <element>GBP</element>
     <element>CAN</element></values>
     <required>False</required>
    </field>
   
    <field name="name_of_government_endorsing_representative" type="zope.schema.TextLine">
      <description>Name of Government Representative endorsing the Project</description>
      <title>Name of Government Endorsing Representative</title>
      <required>False</required>
    </field>
    <field name="post_of_government_endorsing_representative" type="zope.schema.TextLine">
      <description>Post of Government Representative endorsing the Project</description>
      <title>Post of Government Endorsing Representative</title>
      <required>False</required>
    </field>
    <field name="project_activities" type="plone.app.textfield.RichText">
      <description/>
      <title>Project Activities</title>
      <required>False</required>
    </field>
    <field name="project_objectives" type="plone.app.textfield.RichText">
      <description/>
      <title>Project Objectives</title>
      <required>False</required>
    </field>
    <field name="project_outputs" type="plone.app.textfield.RichText">
      <description/>
      <title>Project Outputs</title>
      <required>False</required>
    </field>
    <field name="management_structure" type="zope.schema.Text">
      <description/>
      <title>Management Structure</title>
      <required>False</required>
    </field>
    <field name="stakeholders_and_beneficiaries" type="zope.schema.Text">
      <description/>
      <title>Stakeholders and Beneficiaries</title>
      <required>False</required>
    </field>
    <field name="long_term_sustainability_strategy" type="zope.schema.Text">
      <description/>
      <title>Long-term Sustainability Strategy</title>
      <required>False</required>
    </field>
    <field name="replicability" type="zope.schema.Text">
      <description/>
      <title>Replicability</title>
      <required>False</required>
    </field>
    <field name="monitoring_and_evaluation_process" type="zope.schema.Text">
      <description/>
      <title>Monitoring and Evaluation Process</title>
      <required>False</required>
    </field>
    <field name="funding_sources" type="zope.schema.Text">
      <description/>
      <title>Funding Sources</title>
      <required>False</required>
    </field>
  </schema>
</model>